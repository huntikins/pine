  "scripts": {
    "build": "tsc --project tsconfig.json",
    "build:watch": "build --watch",
    "dev": "nodemon src/pine.ts",
    "format": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@types/get-value": "^3.0.5",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "get-value": "^3.0.1"
  },
  "files": [
